{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name":"Facebook",
      "urls":[
        "https://graph.facebook.com/**"
      ],
      "cacheConfig":{
        "strategy":"freshness",
        "maxAge":"1d",
        "maxSize":20,
        "timeout":"2s"
      }
    },
    {
      "name":"Google",
      "urls":[
        "https://fonts.googleapis.com/**"
      ],
      "cacheConfig":{
        "strategy":"freshness",
        "maxAge":"1d",
        "maxSize":100,
        "timeout":"2s"
      }
    },
    {
      "name":"FirebaseAPI",
      "urls":[
        "https://ksgiflo.firebaseio.com/**"
      ],
      "cacheConfig":{
        "strategy":"freshness",
        "maxAge":"2h",
        "maxSize":100,
        "timeout":"5s"
      }
    }
  ]

}
